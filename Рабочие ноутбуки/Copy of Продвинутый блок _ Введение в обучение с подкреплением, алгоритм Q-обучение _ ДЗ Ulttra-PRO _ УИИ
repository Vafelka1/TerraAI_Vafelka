":["def get_value(self, state):\n","  \"\"\"\n","      Возвращает значение фукнции полезности,\n","      расчитанной по Q[state, action]\n","  \"\"\"\n","  possible_actions = self.get_legal_actions(state)\n","  value = max([self.get_q_value(state, action) for action in possible_actions])\n","  return value\n","\n","QLearningAgent.get_value = get_value"],"metadata":{"id":"vmIzGoWTwVGt","executionInfo":{"status":"ok","timestamp":1641665819611,"user_tz":-180,"elapsed":14,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}}},"execution_count":3,"outputs":[]},{"cell_type":"code","source":["def get_policy(self, state):\n","  \"\"\"\n","      Выбирает лучшее действие, согласно стратегии.\n","  \"\"\"\n","  possible_actions = self.get_legal_actions(state)\n","\n","  best_action = None\n","  \n","  for action in possible_actions:\n","    if best_action is None:\n","      best_action = action\n","    \n","    elif self.get_q_value(state, action) > self.get_q_value(state, best_action):\n","      best_action = action\n","\n","  return best_action\n","\n","QLearningAgent.get_policy = get_policy"],"metadata":{"id":"7FIALqWOw9K0","executionInfo":{"status":"ok","timestamp":1641665819612,"user_tz":-180,"elapsed":14,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}}},"execution_count":4,"outputs":[]},{"cell_type":"code","source":["def get_action(self, state):\n","  \"\"\"\n","      Выбирает действие, предпринимаемое в данном \n","      состоянии, включая исследование. С вероятностью\n","      self.epsilon выполняем случайное действие\n","      согласно стратегии self.get_policy\n","  \"\"\"\n","  possible_actions = self.get_legal_actions(state)\n","\n","  #используем eps-greedy подход\n","  if np.random.random() < self.epsilon:\n","    action = np.random.choice(possible_actions, 1)[0]\n","  else:\n","    action = self.get_policy(state)\n","\n","  return action\n","\n","QLearningAgent.get_action = get_action"],"metadata":{"id":"d06h0bPfwVLY","executionInfo":{"status":"ok","timestamp":1641665819612,"user_tz":-180,"elapsed":14,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}}},"execution_count":5,"outputs":[]},{"cell_type":"code","source":["def update(self, state, action, next_state, reward):\n","  \"\"\"\n","      Функция Q-обновления\n","  \"\"\"\n","  t = self.alpha*(reward + self.discount*self.get_value(next_state) - self.get_q_value(state, action))\n","  reference_qvalue = self.get_q_value(state, action) + t\n","  self.set_q_value(state, action, reference_qvalue)\n","\n","QLearningAgent.update = update"],"metadata":{"id":"bL8zZ5BOzXyo","executionInfo":{"status":"ok","timestamp":1641665819613,"user_tz":-180,"elapsed":14,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}}},"execution_count":6,"outputs":[]},{"cell_type":"code","metadata":{"id":"fHB0ubDJ-heq","executionInfo":{"status":"ok","timestamp":1641665819613,"user_tz":-180,"elapsed":14,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}}},"source":["env = gym.make('Taxi-v3')          #Создаем игровую среду через библиотеку gym\n","\n","n_actions = env.action_space.n"],"execution_count":7,"outputs":[]},{"cell_type":"code","source":["def play_and_train(env, agent, t_max = 10**4):\n","  \"\"\" Функция запускает игру,\n","      использую стратегию агента,\n","      выполняет обновление агента\n","      и отдает общий reward\n","  \"\"\"    \n","  total_raward = 0.\n","  state = env.reset()\n","\n","  for t in range(t_max):\n","      # выбираем действие\n","      a = agent.get_action(state)\n","      next_s , r , done, _ = env.step(a)\n","\n","      # выполняем обновение стратегии\n","      agent.update(state, a, next_s, r)\n","\n","      state = next_s\n","      total_raward += r\n","      if done:\n","        break\n","\n","  return total_raward"],"metadata":{"id":"BQgB6Go6nMgE","executionInfo":{"status":"ok","timestamp":1641665819614,"user_tz":-180,"elapsed":15,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}}},"execution_count":8,"outputs":[]},{"cell_type":"code","source":["agent = QLearningAgent(alpha = 0.5, epsilon = 0.1,\n","                       discount = 0.9,\n","                       get_legal_actions = lambda s: range(n_actions)\n","                       )\n","\n","from IPython.display import clear_output\n","#assert 'get_policy' in dir(agent)\n","rewards= []\n","\n","for i in range(5000):\n","  rewards.append(play_and_train(env,agent))\n","\n","  if i % 100 == 0:\n","    clear_output(True)\n","    print(\n","        \"eps =\", agent.epsilon,\n","        'mean_reward =', np.mean(rewards[-10:])\n","    )\n","    print('alpha=', agent.alpha)\n","    plt.plot(rewards)\n","    plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":299},"id":"riu9-6bSp0dA","executionInfo":{"status":"ok","timestamp":1641665833330,"user_tz":-180,"elapsed":13730,"user":{"displayName":"Vafelka","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11276804272897708630"}},"outputId":"2d9474f9-6928-4f52-aa92-11fd499c83fe"},"execution_count":9,"outputs":[{"output_type":"stream","name":"stdout","text":["eps = 0.1 mean_reward = 0.5\n","alpha= 0.5\n"]},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":["def play(env, agent, t_max = 25):\n","  \"\"\" Функция запускает игру,\n","      использую стратегию агента,\n","      выполняет обновление агента\n","      и отдает общий reward\n","  \"\"\"    \n","  total_raward = 0.\n","  